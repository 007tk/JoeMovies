@model JoeMovies.Models.Customer
@{
    ViewBag.Title = "CustomerDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.Name</h1>
<ul>
    <li>MembershipType: @Model.MembershipType.Name</li>
    @if(Model.BirthDate.HasValue)
    {
        <li>Birthdate: @Model.BirthDate</li>
    }
</ul>